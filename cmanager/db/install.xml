      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id"/>
      </KEYS>
    </TABLE>


    <TABLE NAME="cmanager_config" COMMENT="cmanager_config table retrofitted from MySQL" PREVIOUS="cmanager_comments" NEXT="cmanager_formfields">
